#This bash file is for Mac OS

PATH=$PATH:~/bin
export PATH

#always use
alias cp='cp -r'
alias l.='ls -d .* -G'
alias ll='ls -la -G'
alias ls='ls -G'
alias mv='mv -i'
alias rm='rm -i'
alias vi='vim'

#git command
alias ga='git add'
alias gm='git commit -m'
alias gp='git push origin '
alias gl='git pull origin master'
alias gs='git status'
alias gd='git diff'
alias gb='git branch'
alias gc='git checkout'
alias gn='git checkout -b'
